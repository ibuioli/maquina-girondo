{
  "name": "maquina-girondo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "install_all": "npm install && cd cordova && npm install && cd ..",
    "cordova": "cd cordova && rm -r www && cd .. && ng build --configuration production --build-optimizer --bh ./ --output-hashing=none --output-path cordova/www/",
    "cordova-run-a": "npm run cordova && cd cordova && cordova run android",
    "cordova-build-a": "npm run cordova && cd cordova && cordova build android --release",
    "cordova-run-b": "npm run cordova && cd cordova && cordova run browser",
    "cordova-build-b": "npm run cordova && cd cordova && cordova build browser --release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/forms": "^12.2.16",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "core-js": "^2.4.1",
    "mespeak": "^2.0.2",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular/cli": "^12.2.18",
    "@angular/compiler-cli": "^12.2.16",
    "@angular/language-service": "^12.2.16",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.0.2",
    "tslint": "~6.1.0",
    "typescript": "^4.3.5"
  }
}