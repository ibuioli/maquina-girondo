{
  "name": "maquina-girondo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "install_all": "npm install && cd cordova && npm install && cd ..",
    "cordova": "cd cordova && rm -r www && cd .. && ng build --prod --build-optimizer --bh ./ --output-hashing=none --output-path cordova/www/",
    "cordova-run-a": "npm run cordova && cd cordova && cordova run android",
    "cordova-build-a": "npm run cordova && cd cordova && cordova build android --release",
    "cordova-run-b": "npm run cordova && cd cordova && cordova run browser",
    "cordova-build-b": "npm run cordova && cd cordova && cordova build browser --release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.4",
    "@angular/common": "^7.2.4",
    "@angular/compiler": "^7.2.4",
    "@angular/core": "^7.2.4",
    "@angular/forms": "^7.2.4",
    "@angular/http": "^7.2.4",
    "@angular/platform-browser": "^7.2.4",
    "@angular/platform-browser-dynamic": "^7.2.4",
    "@angular/router": "^7.2.4",
    "core-js": "^2.4.1",
    "mespeak": "^2.0.2",
    "rxjs": "^6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "^7.3.1",
    "@angular/compiler-cli": "^7.2.4",
    "@angular/language-service": "^7.2.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^11.9.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.0.2",
    "tslint": "~5.12.1",
    "typescript": "^3.2.4"
  }
